{
  "name": "serverless-aws-nodejs-es2015-template",
  "version": "0.0.1",
  "description": "Boiler plate starter project for AWS Lambda Node.JS with Serverless Framework 1.0.3. It provide support for ES2015/ES6 through babel and webpack",
  "main": "handler.js",
  "scripts": {
    "build": "node_modules/.bin/webpack",
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register",
    "debug-test": "node_modules/.bin/webpack -d --config webpack.debugtest.config.js && set NODE_ENV=test node_modules/.bin/mocha ./.webpack/test/index.js --debug-brk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ronaldwidha/serverless-aws-nodejs-es2015-template.git"
  },
  "author": "Ronald Widha",
  "license": "SEE LICENSE IN LICENSE.txt",
  "homepage": "https://github.com/ronaldwidha/serverless-aws-nodejs-es2015-template",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.0",
    "graceful-fs": "^4.1.9",
    "mocha": "^3.1.2",
    "serverless-webpack": "^1.0.0-rc.2",
    "webpack": "^1.13.2",
    "webpack-node-externals": "^1.5.4"
  }
}
